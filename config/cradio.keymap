/* MOUSE */

#define U_MOUSE_MOVE_MAX 1000                     /* <--- New max speed setting (default: 600) */
#undef MOVE_UP
#undef MOVE_DOWN
#undef MOVE_LEFT
#undef MOVE_RIGHT
#define MOVE_UP MOVE_Y(-U_MOUSE_MOVE_MAX)
#define MOVE_DOWN MOVE_Y(U_MOUSE_MOVE_MAX)
#define MOVE_LEFT MOVE_X(-U_MOUSE_MOVE_MAX)
#define MOVE_RIGHT MOVE_X(U_MOUSE_MOVE_MAX)

&mmv {
    time-to-max-speed-ms = <500>;             /* <--- How long time until max speed is reached (default: 500) */
};

 &sk {
    release-after-ms = <2000>;
    quick-release;
};

#define en1 0
#define en2 1
#define fi1 2
#define fi2 3
#define jp1 4
#define jp2 5
#define gmn1 6
#define num1 7
#define num2 8
#define nav 9
#define sym1 10
#define sym2 11
#define mouse 12
#define ctrll 13
#define short 14
#define gmn2 15
#define lchng 16

/* MACROS AND BEHAVIOURS */

/ {
    macros {
        acdi: animecards_custom_definition_insertion {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&macro_tap &kp NUBS &kp L &kp I &kp LS(NUBS)>
                ;
        };

        e2m: english_layer_2 {
        compatible = "zmk,behavior-macro";
        #binding-cells = <0>;
        bindings = < &sl en2>;
    };

        f2m: finnish_layer_2 {
        compatible = "zmk,behavior-macro";
        #binding-cells = <0>;
        bindings = < &sl fi2 >;
    };

        j2m: japanese_layer_2 {
        compatible = "zmk,behavior-macro";
        #binding-cells = <0>;
        bindings = < &sl jp2 >;
    };

        n2m: number_layer_2 {
        compatible = "zmk,behavior-macro";
        #binding-cells = <0>;
        bindings = < &sl num2>;
    };

    shft: shift_that_doesnt_give_duplicate_presses {
        compatible = "zmk,behavior-macro";
        #binding-cells = <0>;
        bindings = < &sk LSHFT>;
    };

        c2e: change_to_english {
        compatible = "zmk,behavior-macro";
        #binding-cells = <0>;
        bindings = 
        <&macro_press &kp LGUI>,
        <&macro_tap &kp RIGHT>,
        <&macro_release &kp LGUI>,
        <&macro_tap &to en1>;
    };

        c2f: change_to_finnish {
        compatible = "zmk,behavior-macro";
        #binding-cells = <0>;
        bindings = 
        <&macro_press &kp LGUI>,
        <&macro_tap &kp RIGHT>,
        <&macro_release &kp LGUI>,
        <&macro_tap &to fi1>;
    };

        c2j: change_to_japanese {
        compatible = "zmk,behavior-macro";
        #binding-cells = <0>;
        bindings = 
        <&macro_press &kp LGUI>,
        <&macro_tap &kp RIGHT>,
        <&macro_release &kp LGUI>,
        <&macro_tap &to jp1>;
    };


    };
};

