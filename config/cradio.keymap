/* THE ACTUAL KEYMAP */

    keymap {
        compatible = "zmk,keymap";
        
        en1 {
            bindings = <
        //╭──────────┬──────────┬──────────┬──────────┬──────────╮   ╭──────────┬──────────┬──────────┬──────────┬──────────╮
        //│          │          │          │          │          │   │          │          │          │          │          │
           &none  &hm2 LSHFT P &hm short I &hm21 LALT D &none          &none     &hm21 LALT G &kp C    &hm2 LSHFT R &none
        //├──────────┼──────────┼──────────┼──────────┼──────────┤   ├──────────┼──────────┼──────────┼──────────┼──────────┤
        //│          │          │          │          │          │   │          │          │          │          │          │
